{
  "name": "elysia-nextjs-tanstack-query",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint . --fix && prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:unit": "bun test __tests__/unit",
    "test:integration": "bun test __tests__/integration",
    "bench": "bun run __tests__/benchmark/api-benchmark.ts && bun run __tests__/benchmark/hooks-benchmark.ts",
    "bench:api": "bun run __tests__/benchmark/api-benchmark.ts",
    "bench:hooks": "bun run __tests__/benchmark/hooks-benchmark.ts",
    "bench:quick": "bun run __tests__/benchmark/quick-report.ts",
    "bench:report": "bun run __tests__/benchmark/report.ts"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.4.6",
    "@tanstack/react-query": "^5.90.12",
    "elysia": "^1.4.19",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/bun": "^1.3.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "happy-dom": "^20.0.11",
    "mitata": "^1.0.34",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "ignoreScripts": [
    "sharp",
    "unrs-resolver"
  ],
  "trustedDependencies": [
    "sharp",
    "unrs-resolver"
  ]
}
